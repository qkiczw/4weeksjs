{
  "overrides": [
    {
      "files": "*.json",
      "options": { "parser": "json" }
    },
    {
      "files": "*.html",
      "options": {
        "parser":"html"
      }
    },
    {
      "files": "*.css",
      "options": {
        "parser":"css"
      }
    }
  ],
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "parser": "babel"
}
